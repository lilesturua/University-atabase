CREATE TABLE subjectTimetable (
  id int NOT NULL primary key,
  day varchar(255),
  StartTime time
 );
 CREATE TABLE faculty (
   id int NOT NULL primary key,
   FacultyName varchar(255)
 );
 CREATE TABLE courses (
  id int NOT NULL primary key,
  CourseNumber int
 );
 CREATE TABLE facultyCourses (
  id int NOT NULL primary key,
  faculty_id int,
  courses_id int,
  foreign key (faculty_id)
  references faculty(id),
  foreign key (courses_id)
  references courses(id)
);
 CREATE TABLE subject (
  id int NOT NULL primary key,
  SubjectName varchar(255),
  faculty_id int,
  courses_id int,
  foreign key (faculty_id)
  references faculty(id),
  foreign key (courses_id)
  references courses(id)
 );
 CREATE TABLE address (
  id int NOT NULL primary key,
  SecondAddress varchar(255),
  HouseNumber int
 );
 CREATE TABLE student (
  id int NOT NULL primary key,
  FirstName varchar(255),
  LastName varchar(255),
  email varchar(255),
  personal_id varchar(255),
  Address varchar(255),
  SecondAddress_id int,
  DateOfBirth Date,
  Gender varchar(10),
  Faculty_id int,
  courses_id int,
  foreign key (faculty_id)
  references faculty(id),
  foreign key (courses_id)
  references courses(id),
  foreign key (SecondAddress_id)
  references address(id)
 );
 CREATE TABLE lecturer (
  id int NOT NULL primary key,
  FirstName varchar(255),
  LastName varchar(255),
  email varchar(255),
  personal_id varchar(255),
  Address varchar(255),
  SecondAddress_id int,
  DateOfBirth Date,
  Gender varchar(10),
  BankAccount varchar(255),
  Rank varchar(255),
  foreign key (SecondAddress_id)
  references address(id)
 );
 CREATE TABLE lecturerSubject (
  id int NOT NULL primary key,
  lecturer_id int,
  subject_id int,
  subjectTimetable_id int,
  foreign key (lecturer_id)
  references lecturer(id),
  foreign key (subject_id)
  references subject(id),
  foreign key (subjectTimetable_id)
  references subjectTimetable(id)
 );
 CREATE TABLE manager (
  id int NOT NULL primary key,
  FirstName varchar(255),
  LastName varchar(255),
  Email varchar(255),
  Personal_id varchar(255),
  Address varchar(255),
  SecondAddress_id int,
  DateOfBirth Date,
  Gender varchar(10),
  Faculty_id int,
  foreign key (SecondAddress_id)
  references address(id),
  foreign key (Faculty_id)
  references faculty(id)
 );
 
